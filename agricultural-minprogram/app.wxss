.bg-wrapper {}

.bg-main {
	width: 100%;
	height: 600rpx;
	/* opacity: 1; */
	background: rgba(0, 0, 0, 0.2);
	/* filter: blur(1rpx); */
	display: flex;
	flex-direction: row;
	align-items: center;
}

.bg-main .item-wrapper {
	padding: 0 32rpx;
}

.bg-main .item-wrapper .item-main {
	/* color: red; */
	filter: blur(0rpx);
	display: flex;
	flex-direction: row;

}

.bg-main .item-wrapper .item-main .pic {
	width: 216rpx;
	height: 216rpx;
	background-color: gray;
	border-radius: 24rpx;
	overflow: hidden;
	margin-right: 32rpx;
}

.bg-main .item-wrapper .item-main .intro {
	flex: 1;
}

.bg-main .item-wrapper .item-main .intro .desc {
	font-size: 40rpx;
	font-weight: 600;
	overflow: hidden;
	text-overflow: ellipsis;
	/* 设置为弹性盒子 */
	display: -webkit-box;
	/* 最多显示四行 */
	-webkit-line-clamp: 2;
	/* 弹性盒子的子元素排列方式 */
	-webkit-box-orient: vertical;
	color: rgba(255, 255, 255, 1);
}

.bg-main .item-wrapper .item-main .intro>.count {
	font-size: 24rpx;
	color: rgba(255, 255, 255, 1);
	margin-top: 8rpx;
}

.bg-main .item-wrapper .item-main .intro .join {
	display: flex;
	margin-top: 32rpx;
}

.bg-main .item-wrapper .item-main .intro .join .avatar {
	display: flex;
	margin-right: 16rpx;
}

.bg-main .item-wrapper .item-main .intro .join .avatar .list {
	display: flex;
	position: relative;
	width: 112rpx;
}

.bg-main .item-wrapper .item-main .intro .join .avatar .list .item {
	position: absolute;
	top: 0;
	width: 32rpx;
	height: 32rpx;
	border-radius: 50%;
	overflow: hidden;
	opacity: 1;
	border: 1px solid rgba(255, 255, 255, 1);
}

.bg-main .item-wrapper .item-main .intro .join .avatar .list .item image {
	width: 100%;
	height: 100%;
}

.bg-main .item-wrapper .item-main .intro .join .list .more {
	display: flex;
	align-items: center;
	justify-content: center;
	width: 32rpx;
	height: 32rpx;
	border-radius: 50%;
	opacity: 1;
	border: 1px solid rgba(255, 255, 255, 1);
}

.bg-main .item-wrapper .item-main .intro .join .count {
	font-size: 24rpx;
	font-weight: 400;
	color: rgba(255, 255, 255, 1);
}

